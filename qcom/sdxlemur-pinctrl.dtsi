


&soc {
	tlmm: pinctrl@f100000 {
		compatible = "qcom,sdxlemur-pinctrl";
		reg = <0xf100000 0x300000>;
		reg-names = "pinctrl";
		interrupts = <GIC_SPI 212 IRQ_TYPE_LEVEL_HIGH>;
		gpio-controller;
		#gpio-cells = <2>;
		interrupt-controller;
		interrupt-parent = <&intc>;
		#interrupt-cells = <2>;
	};
};
