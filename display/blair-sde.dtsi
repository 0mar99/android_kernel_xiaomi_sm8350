#include "holi-sde.dtsi"

&mdss_dsi0 {
	vdda-1p2-supply = <&L4A>;
};

&mdss_dsi_phy0 {
	compatible = "qcom,dsi-phy-v4.1";
	reg = <0xae94400 0x800>,
	      <0xae94900 0x27c>,
	      <0xaf03000 0x8>,
	      <0xae94200 0x100>;
	reg-names = "dsi_phy", "pll_base", "gdsc_base", "dyn_refresh_base";
	pll-label = "dsi_pll_5nm";

	qcom,platform-lane-config = [00 00 0a 0a
				     00 00 0a 0a
				     00 00 0a 0a
				     00 00 0a 0a
				     00 00 8a 8a];

	vdda-0p9-supply = <&S1E_LEVEL>;
};

